                                 		INC ZH
                                 .ENDMACRO
                                 
                                 ; Adds the content of a word register to the Z pointer
                                 ; SUMZW <high_register> <low_register>
                                 .MACRO SUMZW
                                 	ADD ZH, @0
                                 	SUMZ @1
                                 .ENDMACRO
                                 
