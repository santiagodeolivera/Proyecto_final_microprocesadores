                                 	CPI @3, 0xFF
                                 	BRNE xor_p1_end
                                 		LSL @4
                                 	xor_p1_end:
                                 	
                                 	MOV @2, @0
                                 	; Make XOR of bytes 2, 3, 6 and 7
                                 	CLR @3
                                 	LSR @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	LSR @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	ORI @3, 0b11111110
                                 	CPI @3, 0xFF
                                 	BRNE xor_p2_end
                                 		LSL @4
                                 		LSL @4
                                 	xor_p2_end:
                                 
                                 	MOV @2, @0
                                 	; Make XOR of bytes 4, 5, 6 and 7
                                 	CLR @3
                                 	LSR @2
                                 	LSR @2
                                 	LSR @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	LSR @2
                                 	EOR @3, @2
                                 	ORI @3, 0b11111110
                                 	CPI @3, 0xFF
                                 	BRNE xor_p4_end
                                 		LSL @4
                                 		LSL @4
                                 		LSL @4
                                 		LSL @4
                                 	xor_p4_end:
                                 
                                 	MOV @2, @0
                                 	EOR @2, @4
                                 
                                 	MOV @4, @2
                                 	ORI @4, 0b11110111
                                 	CPI @4, 0xFF
                                 	BRNE PC+2
                                 		ORI @1, 1
                                 
                                 	MOV @4, @2
                                 	ORI @4, 0b11011111
                                 	CPI @4, 0xFF
                                 	BRNE PC+2
                                 		ORI @1, 2
                                 
                                 	MOV @4, @2
                                 	ORI @4, 0b10111111
                                 	CPI @4, 0xFF
                                 	BRNE PC+2
                                 		ORI @1, 4
                                 
                                 	MOV @4, @2
                                 	ORI @4, 0b01111111
                                 	CPI @4, 0xFF
                                 	BRNE PC+2
                                 		ORI @1, 8
                                 
                                 	POP @4
                                 	POP @3
                                 	POP @2
                                 	POP @0
                                 .ENDMACRO
                                 
                                 
