                                 .MACRO NIBBLETOHAMMING
                                 	PUSH @2
                                 	PUSH @3
                                 
                                 	CLR @2
                                 	CLR @1
                                 
                                 	MOV @2, @0
                                 	ORI @2, 0b11111110
                                 	CPI @2, 0xFF
                                 	BRNE nibble_to_hamming_d3_end
                                 		LDI @3, 0b00001110
                                 		EOR @1, @3
                                 	nibble_to_hamming_d3_end:
                                 
                                 	MOV @2, @0
