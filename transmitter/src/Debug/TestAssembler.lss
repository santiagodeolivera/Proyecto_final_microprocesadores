                                 		LDI @3, 0b00110010
                                 		EOR @1, @3
                                 	nibble_to_hamming_d5_end:
                                 
                                 	MOV @2, @0
                                 	ORI @2, 0b11111011
                                 	CPI @2, 0xFF
                                 	BRNE nibble_to_hamming_d6_end
                                 		LDI @3, 0b01010100
                                 		EOR @1, @3
                                 	nibble_to_hamming_d6_end:
                                 
                                 	MOV @2, @0
                                 	ORI @2, 0b11110111
                                 	CPI @2, 0xFF
                                 	BRNE nibble_to_hamming_d7_end
                                 		LDI @3, 0b10010110
                                 		EOR @1, @3
