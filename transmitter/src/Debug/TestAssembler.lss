                                 	short_to_digits_start:
                                 		CPI @0, high(@2)
                                 			BRLO short_to_digits_end
                                 			BRNE short_to_digits_add
                                 		CPI @1, low(@2)
                                 			BRLO short_to_digits_end
                                 	short_to_digits_add:
                                 		SUBI @1, low(@2)
                                 		BRPL PC+2
